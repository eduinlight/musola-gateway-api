{
  "name": "gateways-api",
  "version": "0.0.1",
  "description": "atiko application",
  "author": "Eduin Garcia Cordero",
  "main": "./dist/main.js",
  "private": true,
  "license": "MIT",
  "scripts": {
    "docker": "docker exec -it gateways-api",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"./**/*.ts\"",
    "start": "npm run start:dev",
    "start:dev": "nest start --watch",
    "start:db": "docker-compose -f docker-compose-db.yml up -d",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest ./src",
    "tdd": "jest --watch ./src",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "migration:create": "ts-node ./src/migrations new -n",
    "migration:up": "ts-node ./src/migrations up",
    "migration:down": "ts-node ./src/migrations down -l",
    "migration:down:all": "ts-node ./src/migrations down -a",
    "migration:status": "ts-node ./src/migrations status",
    "migration:prod:up": "node ./dist/migrations up",
    "migration:prod:down": "node ./dist/migrations down -l",
    "migration:prod:down:all": "node ./dist/migrations down -a",
    "migration:prod:status": "node ./dist/migrations status"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.18",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.18",
    "@nestjs/mongoose": "^7.2.4",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/testing": "^8.2.3",
    "base64-url": "^2.3.3",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.2",
    "dayjs": "^1.10.7",
    "dotenv": "^8.6.0",
    "express-rate-limit": "^5.5.1",
    "helmet": "^4.6.0",
    "joi": "^17.4.2",
    "mongo-migrate-ts": "^1.0.13",
    "mongoose": "^5.13.13",
    "mongoose-autopopulate": "^0.12.3",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rotating-file-stream": "^3.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@types/base64-url": "^2.2.0",
    "@types/bcrypt": "^3.0.1",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^26.0.24",
    "@types/morgan": "^1.9.3",
    "@types/node": "^14.17.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "jest-mock": "^27.3.0",
    "prettier": "^2.5.0",
    "supertest": "^6.1.6",
    "ts-jest": "^26.5.6",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.(e2e-)?spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/./"
    ]
  }
}
